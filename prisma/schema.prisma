datasource db {
  provider = "sqlite"
  url      = "file:../build/sqlite.db"
}

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

enum World {
  KrCarbuncle
  KrChocobo
  KrMoogle
  KrTonberry
  KrFenrir
}

model Server {
  world World @id

  progress    Int
  subprogress Int

  updatedAt DateTime @updatedAt
}
