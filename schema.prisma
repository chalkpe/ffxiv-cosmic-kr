datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client"
  output   = "./prisma"
}

enum World {
  KrCarbuncle
  KrChocobo
  KrMoogle
  KrTonberry
  KrFenrir
}

model Server {
  world World @id

  progress    Int
  subprogress Int

  updatedAt DateTime @updatedAt
}
